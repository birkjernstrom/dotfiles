# Regular aliases
alias ..='cd ..'
alias _='cd -'

# neovim
alias n='nvim'
alias vi='nvim'
alias v='nvim'
alias vim='nvim'

# zoxide override
alias cd='z'

# bat overrides
alias cat='bat'

# exa overrides
alias ls='exa'
alias ll='exa -ahl'
alias tree='exa --tree'

# git
alias g='git'
alias gp='git push'
alias gpo='git push -u origin'
alias gb='git branch'
alias gbd='git branch -d'
alias gs='git status'
alias gc='git commit'
alias gd='git diff'
alias gc='git checkout'
